{
  "name": "fod-generate-sarif",
  "version": "1.1.1",
  "description": "GitHub Action for downloading FPR file from Fortify on Demand",
  "main": "dist/index.js",
  "scripts": {
    "build": "ncc build src/main.ts",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write **/*.ts",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fortify-actions/fod-generate-sarif.git"
  },
  "keywords": [
    "Fortify",
    "FoD",
    "security"
  ],
  "author": "Ruud Senden",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fortify-actions/fod-generate-sarif/issues"
  },
  "homepage": "https://github.com/fortify-actions/fod-generate-sarif#readme",
  "dependencies": {
    "@actions/core": "^1.9.1",
    "fs-extra": "^10.0.0",
    "html-to-text": "^8.0.0",
    "superagent": "^6.1.0",
    "superagent-prefix": "0.0.2",
    "superagent-throttle": "^1.0.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.12",
    "@types/html-to-text": "^8.0.0",
    "@types/node": "^16.9.4",
    "@types/superagent": "^4.1.12",
    "@types/superagent-prefix": "0.0.3",
    "@vercel/ncc": "0.31.1",
    "eslint": "^7.32.0",
    "husky": "^7.0.0",
    "prettier": "^2.4.1",
    "typescript": "^4.4.3"
  }
}
